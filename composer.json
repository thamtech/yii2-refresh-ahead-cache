{
    "name": "thamtech/yii2-refresh-ahead-cache",
    "description": "A Refresh-Ahead Cache Strategy for Yii2",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tyler Ham",
            "email": "tyler@thamtech.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "thamtech/yii2-di": "~0.1",
        "yiisoft/yii2": ">=2.0.14 <2.1"
    },
    "suggest": {
        "yiisoft/yii2-queue": "Required in order to use the QueueGenerator"
    },
    "autoload": {
        "psr-4": { "thamtech\\caching\\refreshAhead\\": "src/" }
    },
    "require-dev": {
        "php": ">=7.2",
        "phpunit/phpunit": "^8.1",
        "yiisoft/yii2-queue": "~2.3.0"
    }
}
